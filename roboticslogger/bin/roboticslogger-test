#!/usr/bin/env python2.7

import time

from roboticslogger.logger import Logger
from multiprocessing import Process, Pipe

myLogger = Logger()

parent_conn, child_conn = Pipe()
p = Process(target=myLogger.run, args=(child_conn,))
p.start()


msg = "wtf"
parent_conn.send(["info", msg])
parent_conn.close()


